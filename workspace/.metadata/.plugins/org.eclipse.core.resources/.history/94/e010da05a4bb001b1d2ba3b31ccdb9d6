package kr.java.homework;

import java.util.Scanner;

public class homework05211 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		if(3 <= n && n <= 10)
		{
			for(int i = 3; i <=0; i--)
			{
				System.out.println("*");
			}
		}
		
		

	}

}
