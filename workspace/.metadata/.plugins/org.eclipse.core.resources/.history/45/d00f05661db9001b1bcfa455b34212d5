package ex.java.input;

import java.util.Scanner;

public class test5 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("숫자를 입력하시오.");
		int num = sc.nextInt();
		for(int i = 0; i<num; i++)
		{
			if(num/i == 1 && num%2 == 1)
			{
				System.out.println(num);
			}
		}
//		if(num*1 == 5)

	}

}
