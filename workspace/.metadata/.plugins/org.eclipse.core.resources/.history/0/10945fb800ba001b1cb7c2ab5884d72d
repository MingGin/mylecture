package kr.java.chapter8;

//class 지시자 : 클래스의 코드를 정의하는 곳!
public class Student {
	
	//멤버 필드(변수, 식별자): 속성
	String address;
	String studentName;
	int grade;
	int studentId;
	
	//멤버 메서드 : 함수
	
	
	
}

class Person {
	
	String name;
	int age;
	boolean sex; // true: 남자, false: 여자.
//	byte s; //s: 0 남자, s: 1 여자 , 나머지 x
	
	
}
