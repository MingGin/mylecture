package kr.java.homework;

import java.util.Scanner;

public class homework05214 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] num = new int[100];
		int sum0 = 0;
		int sum1 = 0;
		int sum2 = 0;
		int sum3 = 0;
		int sum4 = 0;
		int sum5 = 0;
		int sum6 = 0;
		int sum7 = 0;
		int sum8 = 0;
		int sum9 = 0;
		
		System.out.println("자연수를 입력하시오 : ");
		
		for(int i = 0; i < num.length; i++)
		{
			
			num[i] = sc.nextInt();
			if(num[i] == 0)
			{
				for(int j = 0; j < i; j++)
				{
					if(num[j]%10 == 0 && num[j]/10 >= 1)
					{
						sum0++;
						System.out.println("0 : "+sum0);
					}
					else if(num[j]%10 == 1 && num[j]/10 >= 1)
					{
						sum1++;
						System.out.println("1 : "+sum1);
					}
					else if(num[j]%10 == 2 && num[j]/10 >= 1)
					{
						sum2++;
						System.out.println("2 : "+sum2);
					}
					else if(num[j]%10 == 3 && num[j]/10 >= 1)
					{
						sum3++;
						System.out.println("3 : "+sum3);
					}
					else if(num[j]%10 == 4 && num[j]/10 >= 1)
					{
						sum4++;
						System.out.println("4 : "+sum4);
					}
					else if(num[j]%10 == 5 && num[j]/10 >= 1)
					{
						sum5++;
						System.out.println("5 : "+sum5);
					}
					else if(num[j]%10 == 6 && num[j]/10 >= 1)
					{
						sum6++;
						System.out.println("6 : "+sum6);
					}
					else if(num[j]%10 == 7 && num[j]/10 >= 1)
					{
						sum7++;
						System.out.println("7 : "+sum7);
					}
					else if(num[j]%10 == 8 && num[j]/10 < 1)
					{
						sum8++;
						System.out.println("8 : "+sum8);
					}
					else if(num[j]%10 == 9 && num[j]/10 < 1)
					{
						sum9++;
						System.out.println("9 : "+sum9);
					}
					else
					{
						System.out.println("십의자리수가 없습니다.");
					}
					
					
				}
	
				break;
			}
			
		}
		
		

	}

}
