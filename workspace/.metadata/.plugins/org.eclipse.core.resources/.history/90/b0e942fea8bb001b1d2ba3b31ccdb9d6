package kr.java.homework;

import java.util.Scanner;

public class homework05212 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] num = new int[100];
		
		for(int i = 0; i < num.length; i++)
		{
			num[i] = sc.nextInt();
			if(num[i] == 0)
			{
				break;
			}
		}
		
		

	}

}
