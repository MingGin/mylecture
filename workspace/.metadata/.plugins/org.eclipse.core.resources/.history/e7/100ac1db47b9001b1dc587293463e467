package kr.java.homework;

import java.util.Scanner;

public class homework05202 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("수를 입력하시오.");
		int n = sc.nextInt();
		int temp = 0;
		
		for(int i = 0; i<=n; i++)
		{
			for(int j = 0; j<10; j++)
			{
				if(n)
				{
					System.out.println();
				}
				if(j%2==1)
					System.out.print(j+" ");
				temp++;
			}
			
		}
		
		
		

	}

}
