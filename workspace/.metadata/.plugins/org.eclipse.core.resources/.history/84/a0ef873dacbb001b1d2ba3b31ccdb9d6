package kr.java.homework;

import java.util.Scanner;

public class homework05213 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("자연수를 입력하시오 : ");
		int n = sc.nextInt();
		
		for(int i = 1 i < n; i++)
		{
			for(int j = n; j > i; j--)
			{
				System.out.print(" ");
			}
			for(int j = 0; j < i; j++)
			{
				System.out.print(j+1 + " ");
			}
			System.out.println();
		}

	}

}
